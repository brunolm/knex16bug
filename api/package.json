{
  "name": "knex16",
  "version": "1.0.0",
  "description": "knex16 full stack project.",
  "main": "dist/index.js",
  "scripts": {
    "\n# Development": "",
    "dev": "ts-node-dev --respawn --poll --no-notify src/index.ts",
    "\n# Database": "",
    "migrate": "knex migrate:latest --knexfile ./src/db/knexfile.ts",
    "migrate-make": "knex migrate:make --knexfile ./src/db/knexfile.ts",
    "seed": "knex seed:run --knexfile ./src/db/knexfile.ts",
    "seed-make": "knex seed:make --knexfile ./src/db/knexfile.ts",
    "\n# Testing": "",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint --project .",
    "\n# Building": "",
    "build": "tsc -p .",
    "\n# Production": "",
    "start": "node dist/index.js"
  },
  "keywords": [
    "knex16",
    "full-stack-generator"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-async-handler": "^1.1.4",
    "knex": "^0.15.2",
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "^4.1.1",
    "morgan": "^1.9.1",
    "object-casing": "0.0.2",
    "pg": "^7.8.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/knex": "^0.15.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.18",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-consistent-codestyle": "^1.15.0",
    "typescript": "^3.2.2"
  }
}
